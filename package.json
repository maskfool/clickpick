{
  "name": "clickpick",
  "version": "1.0.0",
  "description": "AI-powered YouTube thumbnail maker - Full-stack application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "setup": "chmod +x setup.sh && ./setup.sh"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "backend",
    "frontend"
  ],
  "keywords": [
    "youtube",
    "thumbnail",
    "ai",
    "image-generation",
    "openai",
    "google-ai",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "ClickPick Team",
  "license": "MIT",
  "dependencies": {
    "@uploadthing/react": "^7.3.3",
    "uploadthing": "^7.7.4"
  }
}
